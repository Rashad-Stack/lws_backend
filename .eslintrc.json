{
  "env": {
    "node": true,
    "es6": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:node/recommended",
    "plugin:express/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 2021
  },
  "rules": {
    "semi": ["error", "always"],
    "quotes": ["error", "double"],
    "no-console": "off"
  }
}
